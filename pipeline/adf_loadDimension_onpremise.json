{
	"name": "adf_loadDimension_onpremise",
	"properties": {
		"activities": [
			{
				"name": "Load Dimension",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "dbo.DimProduct"
					},
					{
						"name": "Destination",
						"value": "."
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "daily_game_data_csv",
							"type": "LinkedServiceReference"
						},
						"path": "adf-data"
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ProductKey",
									"type": "Int32"
								},
								"sink": {
									"name": "Player",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "ProductAlternateKey",
									"type": "String"
								},
								"sink": {
									"name": "Time",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "ProductSubcategoryKey",
									"type": "Int32"
								},
								"sink": {
									"name": "Points",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "onprem_dimension_table",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "dimproductcsv",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}